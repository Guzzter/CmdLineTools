<?xml version="1.0" encoding="utf-8" ?>
<VideoThumbnailerConfig>
  <ffmpeg>
    <ffmpeg_path>e:\ffmpeg\ffmpeg.exe</ffmpeg_path>
    <ffprobe_path>e:\ffmpeg\ffprobe.exe</ffprobe_path>
  </ffmpeg>
  <settings
    thumbcount="6"
    thumbquality="75"
    info_json_generation="false"
    subdir="zzzVidPreviews"
    remove_old_first="true"
    first_thumbnail_sec ="10"
    last_thumbnail_sec ="10"
    resursive="true">
  </settings>
  <paths>
    <dir resursive="false" remove_old_first="true">c:\myvideofolder\</dir>
  </paths>
</VideoThumbnailerConfig>
